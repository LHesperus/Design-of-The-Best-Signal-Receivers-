--synthesis_resources = dsp_9bit 2 
SUBDESIGN mult_cms
( 
	clock	:	input;
	dataa[11..0]	:	input;
	datab[13..0]	:	input;
	result[25..0]	:	output;
) 
VARIABLE 
	mac_mult2 : stratixii_mac_mult
		WITH (
			dataa_clear = "0",
			dataa_clock = "0",
			dataa_width = 12,
			datab_clear = "0",
			datab_clock = "0",
			datab_width = 14,
			output_clear = "0",
			output_clock = "0"
		);
	mac_out1 : stratixii_mac_out
		WITH (
			dataa_width = 26,
			dataout_width = 144,
			operation_mode = "output_only",
			output_clear = "0",
			output_clock = "0"
		);
	aclr	: NODE;
	clken	: NODE;

BEGIN 
	mac_mult2.aclr[] = aclr;
	mac_mult2.clk[] = clock;
	mac_mult2.dataa[] = ( dataa[]);
	mac_mult2.datab[] = ( datab[]);
	mac_mult2.ena[] = clken;
	mac_mult2.signa = B"1";
	mac_mult2.signb = B"1";
	mac_out1.aclr[] = aclr;
	mac_out1.clk[] = clock;
	mac_out1.dataa[] = ( mac_mult2.dataout[25..0]);
	mac_out1.ena[] = clken;
	mac_out1.signa = B"1";
	mac_out1.signb = B"1";
	aclr = GND;
	clken = VCC;
	result[25..0] = mac_out1.dataout[25..0];
END;
