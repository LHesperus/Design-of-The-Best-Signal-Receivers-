module	DATA_LATCH
(
	clk,
	datain,
	dataout
);
//////////////////////////////////////////////////////////////////////////////////
//输入输出IO引脚定义//
//////////////////////////////////////////////////////////////////////////////////
input											clk;
input		[13:0]								datain;
output reg	[13:0]								dataout;
/*********************************************************************************/
/************************************程序段开始***********************************/
/*********************************************************************************/ 
	always @(negedge clk) 
begin
	dataout <= datain;
end
/*********************************************************************************/
/************************************程序段结束***********************************/
/*********************************************************************************/                                                                                                                                                                                                                                                                                                                                           
endmodule            